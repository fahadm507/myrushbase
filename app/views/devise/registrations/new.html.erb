<div class="container">
  <div class="row user-log">
    <div class="col-lg-5 col-md-offset-1 user-info-form">
      <div class="form-wrap">
      <h2>Sign up</h2>
        <!-- Button trigger modal -->


      <%=form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="email"><%= f.label :first_name %><br />
      <%= f.text_field :first_name, autofocus: true %></div>

      <div ="email"><%= f.label :last_name %><br />
      <%= f.text_field :last_name, autofocus: true %></div>

      <div ="email"><%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true %></div>

      <div>
        <%= f.label :category_id %>
        <%= f.collection_select(:category_id, Category.all, :id, :name) %>
      </div>
      <div class="i"><%= f.label :password %><br />
        <%= f.password_field :password, autocomplete: "off" %></div>

      <div><%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off" %></div>
      <div class="sign-up-link"><%= f.submit "Sign up", :class => "btn btn-lg btn-success" %></div>
      <% end %>

      <%= render "devise/shared/links" %>
    </div>
    </div>
  </div>
</div>
